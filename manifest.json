{"manifest_version": 3,
"name": "LinkedIn Profile scrapper",
"version": "1.0",
"description": "Open LinkedIn Profile one by one and post scraped data in API",
"permissions": [
    "tabs",
    "scripting" , 
    "storage",
    "activeTab"
],
"host_permissions": [
    "https://www.linkedin.com/*",
    "http://localhost:3000/*"
  ],
"action": {
    "default_popup": "popup.html"},
"background": {
    "service_worker": "background.js"
  }
}